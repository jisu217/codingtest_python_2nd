   "source": "문재 09. 10진수를 2진수로 변환하기",
   "id": "9502a5ca0b162dcb"
  },
  {
   "metadata": {},
   "cell_type": "code",
   "outputs": [],
   "execution_count": null,
   "source": [
    "def solution(decimal):\n",
    "    stack = []\n",
    "    while decimal > 0:\n",
    "        remainder = decimal % 2\n",
    "        stack.append(str(remainder))\n",
    "        decimal //= 2\n",
    "    stack.reverse()\n",
    "    return ''.join(stack)"
   ],
   "id": "initial_id"
  }
